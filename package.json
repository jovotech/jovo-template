{
	"name": "jovo-sample-voice-app-nodejs",
	"version": "4.0.0",
	"description": "A sample voice app that works with the Jovo Framework",
	"main": "./dist/src/index.js",
	"types": "./dist/src/index.d.ts",
	"scripts": {
		"test": "jest",
		"tsc": "tsc",
		"start": "node ./dist/src/app.dev.js",
		"start:dev": "tsc-watch --onSuccess \"npm start\"",
		"bundle:dev": "ncc build src/app.dev.ts -m -s -o bundle/",
		"bundle:prod": "ncc build src/app.prod.ts -s --target es2020 -o bundle/ && cd bundle/ && bestzip ./../bundle.zip index.js index.js.map sourcemap-register.js",
		"eslint": "eslint src test --fix --ext .ts"
	},
	"dependencies": {
		"@jovotech/cli-core": "^4.0.0-alpha.1",
		"@jovotech/db-filedb": "^4.0.0-alpha.2",
		"@jovotech/framework": "^4.0.0-alpha.4",
		"@jovotech/platform-alexa-tmp": "^4.0.0-alpha.3",
		"@jovotech/plugin-debugger": "^4.0.0-alpha.2",
		"@jovotech/server-express": "^4.0.0-alpha.2"
	},
	"devDependencies": {
		"@types/express": "^4.17.11",
		"@types/jest": "^26.0.20",
		"@types/prompts": "^2.0.10",
		"@types/socket.io-client": "^1.4.36",
		"@types/tv4": "^1.2.30",
		"@typescript-eslint/eslint-plugin": "^4.12.0",
		"@typescript-eslint/parser": "^4.12.0",
		"@vercel/ncc": "^0.27.0",
		"bestzip": "^2.1.7",
		"eslint": "^7.17.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"jest": "^26.6.3",
		"prettier": "^2.2.1",
		"ts-jest": "^26.5.3",
		"tsc-watch": "^4.2.9",
		"typescript": "^4.2.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jovotech/jovo-sample-voice-app-nodejs.git"
	},
	"author": "jovotech",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/jovotech/jovo-sample-voice-app-nodejs/issues"
	},
	"homepage": "https://github.com/jovotech/jovo-sample-voice-app-nodejs#readme"
}
