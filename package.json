{
	"name": "jovo-sample-voice-app-nodejs",
	"version": "4.0.0",
	"description": "A sample voice app that works with the Jovo Framework",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"test": "jest",
		"tsc": "tsc",
		"start:dev": "tsc-watch --onSuccess \"node ./dist/app.dev.js --jovo-webhook\"",
		"bundle:dev": "esbuild src/app.dev.ts --bundle --minify --sourcemap --platform=node --outfile=bundle/index.js",
		"eslint": "eslint src test --fix --ext .ts"
	},
	"dependencies": {
		"@jovotech/cli-core": "^4.0.0-beta.1",
		"@jovotech/db-filedb": "^4.0.0-alpha.17",
